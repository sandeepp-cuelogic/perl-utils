pipeline {
  agent any
  stages {
    stage('Build') {
      steps {
        sh 'echo "This is build stage"'
      }
    }
    stage('Test') {
      steps {
        sh 'echo "This is test stage"'
      }
    }
    stage('Deploy') {
      steps {
        sh 'echo "This is test stage"'
      }
    }
  }
}